{
  "name": "flux-todomvc",
  "version": "1.0.0",
  "scripts": {
    "start": "watchify -o js/bundle.js -v -d js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js"
  },
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "envify": "^0.2.0",
    "reactify": "^1.1.1",
    "uglify-js": "^2.4.24",
    "watchify": "^3.3.1"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "envify": "^0.2.0",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "object-assign": "^4.0.1",
    "react": "^0.13.3"
  }
}
